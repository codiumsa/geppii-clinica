	     checked=alimentacion.coordinacion.solidos}}
	No {{view Bodega.views.RadioButton value='no' selectionBinding="solidos"
	     checked=alimentacion.coordinacion.solidos}}
      </div>
      <div class="col-lg-3">
	{{! <label class="form-label" for="lengua">Lengua</label> }}
	<label class="form-label" for="broncoaspiracion">Broncoaspiracion</label>
	{{view Ember.Select 
	  content=coordinacionesPresentes
	  selection=alimentacion.coordinacion.broncoaspiracion
	  prompt="Seleccione un valor..."
	  value=alimentacion.coordinacion.broncoaspiracion
	  classNames="form-control"}}	
      </div>
      <div class="col-lg-3">
	{{! <label class="form-label" for="lengua">Lengua</label> }}
	<label class="form-label" for="regurgitacion">Regurgitacion</label>
	{{view Ember.Select 
	  content=coordinacionesPresentes
	  selection=alimentacion.coordinacion.regurgitacion
	  prompt="Seleccione un valor..."
	  value=alimentacion.coordinacion.regurgitacion
	  classNames="form-control"}}	
      </div>
      <div class="col-lg-3">
	{{! <label class="form-label" for="lengua">Lengua</label> }}
	<label class="form-label" for="sialorrea">Sialorrea</label>
	{{view Ember.Select 
	  content=coordinacionesPresentes
	  selection=alimentacion.coordinacion.sialorrea
	  prompt="Seleccione un valor..."
	  value=alimentacion.coordinacion.sialorrea
	  classNames="form-control"}}
      </div>
      <div class="col-lg-3">
	{{! <label class="form-label" for="lengua">Lengua</label> }}
	<label class="form-label" for="escapeAlimento">Escape de Alimento</label>
	{{view Ember.Select 
	  content=coordinacionesPresentes
	  selection=alimentacion.coordinacion.escapeAlimento
	  prompt="Seleccione un valor..."
	  value=alimentacion.coordinacion.escapeAlimento
	  classNames="form-control"}}	
      </div>
    </div>
  </div>
</div>
