<h3>
  <i class="fa fa-angle-right"></i>
  {{formTitle}}
</h3>
<div class="row mt"  onkeydown="Bodega.ignoreEnter();">
  <div class="col-lg-12">
    <div class="form-panel">
      <div class="form-horizontal">
        <form class="style-form" role="form" {{action "save" on="submit"}}>
          {{ partial "consultas/new/alta_consulta" }}
          <div class="form-group">
            <div class="col-lg-12">
              <button type="submit" class="btn btn-theme">Guardar</button>
              {{#if atendiendo}}<button {{action 'saveImprimir'}} class="btn btn-theme">Guardar e imprimir Receta</button>{{/if}}
              <button {{action 'cancel'}} class="btn btn-default">Cancelar</button>
            </div>

          </div>
        </form>
      </div>
    </div>
  </div>
</div>
