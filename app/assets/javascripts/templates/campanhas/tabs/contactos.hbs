<div role="tabpanel" class="tab-pane" id="contactos">
	<div class="row mt">
		<div class="col-lg-12">
			<div class="content-panel table-responsive" style="padding: 0px;">
				<table class="table table-bordered table-striped table-condensed">
					<thead>
						<tr>
							<th>Patrocinador</th>
							<th>Entregado/Comprometido - In kind</th>
							<th>Estado</th>
						</tr>
					</thead>
					<tbody>
						{{#each contacto in contactos}}
						<tr>
							<td>{{contacto.sponsor.persona.razonSocial}}</td>
							<td>{{{contacto.compromisoPagadoInKind}}}</td>
							<td>{{contacto.estadoActual}}</td>
						</tr>
						{{/each}}
					</tbody>
				</table>
			</div>
		</div>
	</div>
	<div class="form-group">
		<div class="col-lg-12">
			<button type="submit" class="btn btn-theme">Guardar</button>
			<button {{action 'cancel'}} class="btn btn-default">Cancelar</button>
		</div>
	</div>
</div>
